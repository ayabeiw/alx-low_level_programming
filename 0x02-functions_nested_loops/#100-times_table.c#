#include "holberton.h"

/**
 * print_times_table -prints the times table for the number input
 *@n: The times table value
 */

void print_times_table(int n);

#include <stdio.h>
int main() {
  int n, i;
  printf("Enter an integer: ");
  scanf("%d", &n);
  for (i = 1; i <= 10; ++i) {
    printf("%d * %d = %d \n", n, i, n * i);
  }
  return 0;
}